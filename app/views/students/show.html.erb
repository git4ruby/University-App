<div class="row">
  <div class="col s16 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Student Name: <%= @student.name%></span>
        <p>Student Email: <%=@student.email %></p>
        <p>Courses Enrolled: None</p>
      </div>
    </div>
    <%= link_to '<i class="material-icons right">create</i>Edit your profile'.html_safe, edit_student_path(@student), class: "waves-effect waves-light btn" %>
  </div>
</div>
